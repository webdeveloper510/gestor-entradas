CREATE\040TABLE\040Discoteca\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040nombre\040VARCHAR(50)\040NOT\040NULL,
\040\040\040\040lugar\040VARCHAR(50)\040NOT\040NULL,
\040\040\040\040aforo\040INT\040NOT\040NULL
);
CREATE\040TABLE\040Discoteca\040(\040\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,\040\040\040\040\040nombre\040VARCHAR(50)\040NOT\040NULL,\040\040\040\040\040lugar\040VARCHAR(50)\040NOT\040NULL,\040\040\040\040\040aforo\040INT\040NOT\040NULL\040);
CREATE\040TABLE\040Evento\040(
\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,
\040\040\040\040id_disco\040INT\040NOT\040NULL,
