CREATE\040VIEW\040rrpp_ventas\040AS
SELECT\040RRPP.id,\040RRPP.nombre,\040COUNT(Venta.id)\040as\040total_ventas
FROM\040RRPP
LEFT\040JOIN\040Venta\040ON\040RRPP.id\040=\040Venta.id_rrpp
GROUP\040BY\040RRPP.id,\040RRPP.nombre;
CREATE\040VIEW\040rrpp_ventas\040AS\040SELECT\040RRPP.id,\040RRPP.nombre,\040COUNT(Venta.id)\040as\040total_ventas\040FROM\040RRPP\040LEFT\040JOIN\040Venta\040ON\040RRPP.id\040=\040Venta.id_rrpp\040GROUP\040BY\040RRPP.id,\040RRPP.nombre;
SELECT\040*\040FROM\040rrpp_ventas;
