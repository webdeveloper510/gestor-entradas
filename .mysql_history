\040\040\040\040descripcion\040VARCHAR(100)\040NOT\040NULL,
\040\040\040\040aforo\040INT\040NOT\040NULL,
\040\040\040\040FOREIGN\040KEY\040(id_disco)\040REFERENCES\040Discoteca(id)
);
CREATE\040TABLE\040Evento\040(\040\040\040\040\040id\040INT\040AUTO_INCREMENT\040PRIMARY\040KEY,\040\040\040\040\040id_disco\040INT\040NOT\040NULL,\040\040\040\040\040nombre\040VARCHAR(50)\040NOT\040NULL,\040\040\040\040\040descripcion\040VARCHAR(100)\040NOT\040NULL,\040\040\040\040\040aforo\040INT\040NOT\040NULL,\040\040\040\040\040FOREIGN\040KEY\040(id_disco)\040REFERENCES\040Discoteca(id)\040);
